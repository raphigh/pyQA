   "metadata": {},
   "outputs": [],
   "source": [
    "n = int(input('insert a 2digits number please : '))"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": null,
   "metadata": {},
   "outputs": [],
   "source": [
    "n1 = n % 10\n",
    "n2 = n // 10"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": null,
   "metadata": {},
   "outputs": [],
   "source": [
    "print('the reverse is : ','%d%d'%(n1,n2))\n",
    "print('the sum of digits is : ',n1 + n2)"
   ]
  }
